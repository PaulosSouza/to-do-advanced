{
  "name": "to-do-the-new-way",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsx watch src/infra/server.ts",
    "start:prd": "node dist/src/infra/server.js",
    "build": "swc src --ignore **/*.spec.ts,**/*.e2e-spec.ts -d dist",
    "typeorm": "tsx -r dotenv-flow/config -r tsconfig-paths/register node_modules/.bin/typeorm",
    "typeorm:migration:create": "sh src/scripts/create-migration.sh",
    "typeorm:migration:run": "sh src/scripts/run-migration.sh",
    "typeorm:migration:revert": "sh src/scripts/revert-migration.sh",
    "ts:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@shopify/eslint-plugin": "^45.0.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.8",
    "@types/supertest": "^6.0.2",
    "husky": "^9.0.11",
    "supertest": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fastify/awilix": "^5.1.0",
    "@fastify/jwt": "^8.0.1",
    "awilix": "^10.0.2",
    "bcrypt": "^5.1.1",
    "dotenv-flow": "^4.1.0",
    "fastify": "^4.28.0",
    "fastify-zod": "^1.4.0",
    "mongoose": "^8.4.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  }
}
